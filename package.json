{
  "name": "coc-markmap",
  "version": "0.7.1",
  "description": "Visualize your Markdown as mindmaps with Markmap",
  "author": "Gerald <gera2ld@live.com>",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "dev": "rollup -cw",
    "clean": "del-cli dist",
    "prepublishOnly": "run-s build",
    "ci": "run-s lint",
    "build:js": "rollup -c",
    "build": "run-s ci clean build:js",
    "lint": "eslint --ext .ts . && prettier -c src",
    "lint:fix": "eslint --ext .ts . --fix && prettier -c src -w"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@gera2ld/plaid": "^2.7.0",
    "@gera2ld/plaid-rollup": "^2.7.0",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^20.10.5",
    "coc.nvim": "0.0.83-next.9",
    "del-cli": "^5.1.0",
    "husky": "^8.0.3"
  },
  "dependencies": {
    "lodash.debounce": "^4.0.8",
    "markmap-cli": "^0.15.9-alpha.16",
    "portfinder": "^1.0.32"
  },
  "engines": {
    "coc": ">=0.0.80",
    "node": ">=18"
  },
  "keywords": [
    "coc.nvim",
    "markmap"
  ],
  "activationEvents": [
    "onLanguage:markdown"
  ],
  "contributes": {
    "configuration": {
      "title": "coc-markmap",
      "properties": {}
    }
  },
  "repository": "git@github.com:gera2ld/coc-markmap.git",
  "browserslist": [
    "node >= 18"
  ]
}
